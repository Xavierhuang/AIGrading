{
  "version": 2,
  "builds": [
    {
      "src": "rag_grading_ui.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/rag_grading_ui.py"
    }
  ],
  "functions": {
    "rag_grading_ui.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "PYTHON_VERSION": "3.12"
  }
} 